class Car:
    def __init__(self, make, model, year):
        self.make = make
        self.model = model
        self.year = year

    def display_info(self):
        print("Make is:", self.make )
        print("Model is:", self.model)
        print("Year is:", self.year)

car = Car("Dodge", "Durango", "2011")
car.display_info()
